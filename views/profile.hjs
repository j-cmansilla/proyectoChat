<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <script src="/javascripts/jquery-3.2.1.min.js"></script>
    <script src="/javascripts/jquery.tabledit.min.js"></script>
    <script src="/javascripts/jquery.tabledit.js"></script>
    <script src="/javascripts/profile.js"></script>
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
    
    <link rel="stylesheet" href='/stylesheets/style.css'>
  </head>
  <body>
    <h1>{{ title }}</h1>
    <h1>Welcome {{ userName }}</h1>
    <h1 hidden  id="userName">{{userName}}</h1>
    <div class="container">
        <h1>Users:</h1>
        <div class="list-group" id="listaUsuarios">
        </div>
        <h1>Chat:</h1>
        <h1 hidden id="userToSend"></h1>
        <div class="list-group" id="listaDeChats">
        </div>
    </div>
    <div class="container">
        <textarea rows="4" readonly id="messages" cols="50">
             
        </textarea>
        <input type="text" class="form-control" id="messageToSend">
        <button type="button" onclick="sendMessage()" class="btn btn-primary">Secondary</button>
    </div>
    <h1 id="userNameLogin"></h1>
  </body>
</html>